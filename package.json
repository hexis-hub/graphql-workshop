{
  "name": "talk-graphql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "start:watch": "onchange -i -k 'server/**/*.js' -- yarn start",
    "precommit": "NODE_ENV=production lint-staged",
    "start:dev": "concurrently \"yarn start:watch\" \"npm run --prefix client start\""
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint server/ --fix",
      "eslint client/ --fix",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.2.1",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "concurrently": "^4.1.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^10.2.11",
    "onchange": "^5.2.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  }
}
